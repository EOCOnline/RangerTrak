{
  "name": "rangertrak",
  "version": "0.11.38",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "server": "http-server -p 8080 -c-1 dist/rangertrak",
    "release": "standard-version",
    "compodoc": "npx compodoc -p tsconfig.doc.json --theme=Vagrant",
    "style": "import-sort -l \"**/{src,tests,e2e}/**/*.ts\" && prettier --check \"**/{src,tests,e2e}/**/*.{*css,ts}\"",
    "style:fix": "import-sort --write \"**/{src,tests,e2e}/**/*.ts\" && prettier --write \"**/{src,tests,e2e}/**/*.{*css,ts}\" && js-beautify \"**/src/**/*.html\"",
    "lint": "run-p lint:ts lint:tsc",
    "lint:fix": "run-s lint:ts:fix lint:tsc",
    "lint:ts": "tslint --config tslint.json --project . -e \"**/{test,polyfills}.ts\"",
    "lint:ts:fix": "tslint --config tslint.json --fix --project . -e \"**/{test,polyfills}.ts\"",
    "lint:tsc": "tsc --noEmit --skipLibCheck",
    "init:env": "init-dev-env generate-dot-env example.env -f"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/color-picker": "^7.0.1",
    "@angular-material-components/datetime-picker": "^7.0.1",
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.0",
    "@angular/cli": "^13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "^13.3.0",
    "@angular/fire": "^7.2.1",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^13.3.0",
    "@angular/google-maps": "^13.3.0",
    "@angular/material": "^13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@angular/service-worker": "^13.3.0",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-brands-svg-icons": "^6.1.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@googlemaps/markerclusterer": "^2.0.1",
    "@material/banner": "^13.0.0",
    "@material/slider": "^13.0.0",
    "@material/switch": "^13.0.0",
    "@mdi/js": "^6.5.95",
    "@ngx-pwa/local-storage": "^13.0.2",
    "@popperjs/core": "^2.11.4",
    "@what3words/api": "^4.0.5",
    "ag-grid-angular": "^27.1.1",
    "ag-grid-community": "^27.1.0",
    "dayjs": "^1.11.0",
    "firebase": "^9.6.9",
    "idb": "^7.0.1",
    "ngx-dayjs": "^1.0.1",
    "picocolors": "^1.0.0",
    "rxfire": "^6.0.3",
    "rxjs": "^7.5.5",
    "tslib": "^2.3.1",
    "tsviz-cli": "^2.0.3",
    "typescript": ">=4.4.2 and <4.6.0",
    "xlsx": "^0.18.4",
    "xlsx-style": "^0.8.13",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@types/google.maps": "^3.48.2",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^17.0.22",
    "@types/supercluster": "^7.1.0",
    "cross-conf-env": "^1.2.1",
    "dev-norms": "^1.7.1",
    "http-server": "^14.1.0",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "init-dev-env": "^1.0.0",
    "jasmine-core": "~4.0.1",
    "js-beautify": "^1.14.0",
    "jshint": "^2.13.4",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.13.10",
    "tslint-plugin-prettier": "^2.3.0"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module",
      "options": {}
    }
  }
}
